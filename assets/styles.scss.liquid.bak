}
.featured-category-home-section .link-promo .arrow:before {
  content: '';
  border-top: 2px solid #333333;
  border-right: 2px solid #333333;
  height: 6px;
  width: 6px;
  position: absolute;
  right: 0px;
  top: 0;
  bottom: 0;
  margin: auto;
  -webkit-transform:rotate(45deg) translate(1px, -1px);
  transform:rotate(45deg) translate(1px, -1px);
  transition:0.3s;
}
.featured-category-home-section .link-promo .arrow:after {
  content: '';
  border-bottom: 2px solid #333333;
  height: 0px;
  width: 16px;
  position: absolute;
  right: 0;
  top: 0px;
  bottom: 0;
  margin: auto;
  transition:0.3s;
}
