      display: block;
      position: static;
    }
  }

  {% if settings.thumbnail_hover_enabled %}

  .product-wrap {
    text-align: center;
    position: relative;
    &:hover .collection-details {
      opacity: 1;
      transform: none;
    }
    &:hover .view_all {
      opacity: 1;
      transform: none;
    }
    &:hover .thumbnail-overlay {
      opacity: 1;
    }
  }

  .thumbnail-overlay {
    .info {
      @include vertical-alignment;
      position: absolute;
      text-align: center;
      width: 100%;
      margin: 0 auto;
