  width: 150px;
  height: 50px;
  position: absolute;
  right: 10px;
  top: 10px;
  border: 1px solid #000;
  font-size: 14px;
}

.overlay-data {
  opacity: 0;
  visibility: hidden;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
  visibility: 0s 0.5s;
  transition: opacity 0.5s, visibility 0s 0.5s;
}

.overlay-open {
  opacity: 1;
  visibility: visible;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
}

.inputbox {
  margin: 0px auto;
