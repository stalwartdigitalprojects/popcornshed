        </div>
        {% if block.settings.enable_captcha == true %}<div class="sixteen columns"><div id="re-captcha"></div></div>{% endif %}

        <input type="submit" class="submit action_button" value="{{ 'contact.form.send' | t }}" {% if block.settings.enable_captcha == true %}disabled{% endif%}/>
        {% endform %}
        
        <form style="display:none;" action="{{'https://popcornshed.us13.list-manage.com/subscribe/post-json?u=ccd12e7be94cc008a2de60e1d&amp;id=6898c7e4c3&c=?'}}" method="post" id="gifiting-register-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
