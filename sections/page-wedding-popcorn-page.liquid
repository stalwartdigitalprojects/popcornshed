            <option value="">-- Select --</option>
            {% for msg in hear_about %}<option value="{{ msg | strip | strip_html }}">{{msg | strip | strip_html}}</option>{% endfor %}
            </select>
            {% endif %}
        </div>

        <div class="form_chkbx">
          <input type="checkbox" id="ContactFormchkbx1" name="contact[mailing_list]" value="Mailing List1" checked="checked" /> 
