          <input type="text" id="contactFormFirstName" name="contact[{{ fname_attr }}]"  autocapitalize="words" value="{% if form[fname_attr] %}{{ form[fname_attr] }}{% elsif customer %}{{ customer.firstname }}{% endif %}" required="required">
