{%- include 'bold-cart' -%}
{% comment %}
** Cart page - default view **
- Template section
{% endcomment %}

<a name="pagecontent" id="pagecontent"></a>

<script>
 
</script>

<div class="section clearfix">
  <div class="sixteen columns margin-top10">
    <h1 class="majortitle center margin-top20">{{ 'cart.general.title' | t  | upcase}}</h1>
    {% assign final_cart_count = cart.item_count %}
    {% if cart.item_count > 0 %}
      {% assign fsb_val = 0%}
      {% assign fsb_limit = settings.fsb_shipping_limit | times: 100 %}
    {% if bold_cart_total_price > fsb_limit %}
    	<h3 class="shipping-title center">
        Congratulations! You get free shipping
      </h3>
    {% else %}
    	{% assign fsb_val = fsb_limit | minus: bold_cart_total_price  %}
      <h3 class="shipping-title center">
        Add {{ fsb_val | money }} to be eligible for Free Shipping      
      </h3>
      {% endif %}
    
      {%comment%}
      {% if bold_cart_total_price > fsb_limit %}
    	<h3 class="shipping-title center">
        Congratulation! You get free shipping
      </h3>
      {% else %}
    	{% assign fsb_val = fsb_limit | minus: bold_cart_total_price  %}
      <h3 class="shipping-title center">
        Add {{ fsb_val | money }} to to eligible for Free Shipping      
      </h3>
      {% endif %}
    {% endcomment %}
    {% endif %}
  </div>
</div>


{% if cart.item_count == 0 %}
<div class="sixteen columns">
  <div class="section clearfix">
    <div class="eleven columns offset-by-five margin-bottom100">
      <p class="quote">{{ 'cart.general.continue_browsing_html' | t }}</p>
      <a href="{% if cart.items.first.product.collections != blank %}{{ cart.items.first.product.collections.last.url }}{% else %}/collections/all{% endif %}" class="action_button  highlight-false add_to_cart">{{ 'cart.general.continue_shopping_link_html' | t }}</a>
    </div>
    <br class="clear" />
  </div>
</div>
{% else %}
<div class="template-cart">  
  <form action="/cart" method="post" id="cart_form" class="page-cart"  data-money-format="{{ shop.money_format }}" data-shop-currency="{{ shop.currency }}" data-shop-name="{{ shop.name | escape }}">

    
    <div class="upper-checkout-button" style="float:right;">
      <div class="top_content">
        <div class="top-right-content">
          <div id="ms__toggle-box">
            <p>
              <input type="checkbox" id="ms__toggle-shipping" value="Ship to Multiple"> <label for="ms__toggle-shipping">Ship to multiple addresses</label>
            </p>
          </div>
        </div>
        <div class="right_content">
          <a href="javascript:;" id="empty_basket" class="action_button">Empty My Basket</a>
        </div>
      </div>
      <div class="bottom_content">
        <p class="bundle_message">{{settings.bundle_warning}}</p>
        <div class="bottom-right-content">
          <div class="delivery_date_wrapper">
            <label for="cart_datepicker">Pick your dispatch date</label>
            <input data-toggle="datepicker" id="cart_datepicker" autocomplete="chrome-off" name="attributes[Delivery Date]"  placeholder="DD/MM/YYYY" value="{{cart.attributes['Delivery Date']}}" />
            <input type="hidden" name="attributes[Delivery Day]" id="delivery_day" class="uiDeliveryDayVal" value="{{cart.attributes['Delivery Day']}}">
          </div>
        </div>
        
        <p>
          {{ bold_ro_cart }}
          <input type="submit" class="checkout-btn"  id="checkout" name="checkout" value="{{ 'cart.general.checkout' | t | upcase}}" />        
        </p>
      </div>
    </div> 
    <ul class="item-list divider clearfix ">
      {% assign total_saving = 0 %}
      {% assign original_price = 0 %}
      {% assign has_bundles = false %}
      
      {% assign bundle_types = blank %}
      
      {% for item in cart.items %}
<!--       {{item.variant.compare_at_price}}-{{item.variant.price}} -->
       {% if item.variant.compare_at_price > item.variant.price %}
            {% assign saving = item.variant.compare_at_price | minus: item.variant.price | times: item.quantity %}
            {% endif %}
      {% assign pro_size = item.properties | size %}
      {% assign Hasbundle_type = false %}
          {% for property in item.properties %}
      		{% if property.first == "bundle_type" %}
          		{% assign Hasbundle_type = true %}
          		{% break %}
          	{% endif %}
          {% endfor %}
      {% if Hasbundle_type == false %}
      {% capture normal_items %}
      	{{normal_items}}
      	{% include 'single-cart-item', type:"cart" %}
      {% endcapture %}
      {% endif %}
      	{% assign q = item.quantity %}
      {% for property in item.properties %}
      {% if property.first == "bundle_type" %}
      
      	{% if property.last contains "bundle-group-" %}{% capture bundle_types %}{{bundle_types}}|{{property.last}}{% endcapture %}{% endif %}
      {% endif %}
      {% endfor %}
      
      {%- assign bundle_types_array = bundle_types | split: '|' | uniq -%}
      {% endfor %}
      
      {% for bund in bundle_types_array %}
      {% assign line_price = blank %}
      {% assign line_original_price = blank %}
      {% assign temp_arr = blank %}
      {% assign var_ids = blank %}
      {% if bund != blank %}
      {% assign bund_grp = blank %}
      {% capture bund_grp %}{{bund_grp}}<div>
      {% assign bundle_handle = bund | split:":" | first |remove:"bundle-group-" %}
      {% assign bundle_obj = pages[bundle_handle] %}
      {% assign bundle_img = bundle_obj.metafields.custom_fields.bundle_builder_image_url %}
      {% assign bundle_name = bundle_obj.metafields.custom_fields.bundle_builder_group_name %}
      <li class="item cf midnight-thunder">
        <div class="image bundle_prod_image">
          <a href="javascript:;">
            {% if bundle_img != blank %}
            <img src="{{bundle_img}}" title="{{bund}}"/>
            {% else %}
            <img src="https://cdn.shopify.com/s/files/1/1354/3525/files/Build_a_bundle_Snack_Packs_600x600_Tekengebied_1_kopie-min.png?v=1610027667" title="{{bundle_name}}"/>
            {% endif %}
          </a>
        </div>
        <div class="bundle_wrapper">
        <div class="prise-detail">
          <a class="name" href="javascript:;">{{bundle_name}}</a>          
          <div class="subitems_wrapper">            
           {% for item in cart.items %}
              {% for property in item.properties %}
                  {% if bund == property.last %}
            		{% assign line_price = line_price | plus: item.final_line_price %}
            		{% assign line_original_price = line_original_price | plus:item.original_line_price %}
            		{% capture temp_arr %}{{temp_arr}}img:{{item | img_url: '100x'}},handle:{{item.handle}},var_title:{{item.title}},prod_title:{{item.product.title}},original_line_price:{{item.original_line_price | money_without_currency }},line_level_discount_allocation_amount:{{item.line_level_discount_allocations[0].amount}},qty:{{item.quantity}},var_id:{{item.variant.id}}~{% endcapture %}
            		{% comment %}{% include 'single-cart-item',item:item %}{% endcomment %}
                  {% endif %}
              {% endfor %}
            {% endfor %}
            {% assign bunds = temp_arr | split:'~' %}
            {% for single_bun in bunds %}
            {% assign split_bund = single_bun | split:',' %}
            <div class="subitem">
              {% for keyval in split_bund %}
                {% assign key = keyval | split:':' | first %}
                {% assign val = keyval | split:':' | last %}
                {% if key == "img" %}
                <div class="product-img">
                  <img src="{{ val }}" />
                </div>
                {% elsif key == "qty" %}
                <span class="qty">{{val}}</span>
                {% elsif key == "var_title" %}
                <span class="item_title">{{val}}</span>
              {% elsif key == "var_id" %}
              {% assign var_ids = var_ids | append:val | append:"," %}
                {% endif %}
              {% endfor %}
            </div>
            {% endfor %}
            </div>
          <div class="bundle_wrapper_bottom">
            {% assign var_ids_size = var_ids | size | minus: 1 %}
            <div class="bundle-remove"><a href="javascript:;" data-ids="{{var_ids | slice:0,var_ids_size}}">Delete</a></div>
            <div class="bundle_price"> <span class="money">{% if line_original_price != blank %}<strike>{{line_original_price | money}}</strike>&nbsp;{% endif %}{{line_price | money}}</span></div>
          </div>
        </div>
        </div>
      </li>
      </div>
      {% endcapture %}
      {{bund_grp}}
      {% endif %}
      {% endfor %}
      
      {{normal_items}}
      
    </ul> 
    <div class="gift-info-note">
    <div class="gift-info-note-left">
      {% if section.settings.display_special_instructions %}
      <label for="note">{% if section.settings.cart_note_label != blank %}{{ section.settings.cart_note_label }}{% else %}{{  'cart.general.note' | t: shop_name: shop.name }}{% endif %}:</label>
      <textarea id="note" name="note" rows="2" style="resize:none;" maxlength="255" placeholder="Your message here (Max. 255 character)">{{ cart.note }}</textarea>
      {% endif %}
    </div>
      
    <div class="follow-scroll" style="text-align:right;">
      
      <p class="js-cart_subtotal" style="margin-bottom:10px;">
        {% assign discount_size = cart.discount_applications | size %}
        {% if total_saving > 0%}
        
<!--         <span class="was_price_clr original_price" style="font-size: 23px;display: block;line-height: 26px;font-weight: 400;">{{ 'Was:' | upcase }} {{ original_price | money }}</span>         -->
        <span class="majortitle">{{ 'Now' | upcase }}:
          <span class=""><span class="wh-original-cart-total"><span id="">
            <span class="saso-cart-original-total total-sale">
              <strike>{{ bold_cart_total_price  | plus:total_saving | money }}</strike> {{cart.total_price | money }}

            </span>
            <span class="saso-cart-total"></span>
            </span></span><span class="wh-cart-total"></span><div class="additional-notes"><span class="wh-minimums-note"></span><span class="wh-extra-note"></span></div>
          </span>
        </span>
        {% elsif discount_size > 0 %}
        <span style="font-size:18px !important;"><span id="">{{ 'cart.general.subtotal' | t | upcase }}</span>: <span class=""><span class="wh-original-cart-total"><span id="">
          <span class="saso-cart-original-total discount_size">
            <strike>{{ bold_cart_total_price | money }}</strike> {{cart.total_price | money }}
          </span>
          <span class="saso-cart-total"></span>
          </span></span><span class="wh-cart-total"></span><div class="additional-notes"><span class="wh-minimums-note"></span><span class="wh-extra-note"></span></div></span>
        </span>
        {% else %}
        <span style="font-size:18px !important;"><span id="">{{ 'cart.general.subtotal' | t | upcase }}</span>: <span class=""><span class="wh-original-cart-total"><span id="">
          <span class="saso-cart-original-total else">
            {{cart.total_price | money }}
          </span>
          <span class="saso-cart-total"></span>
          </span></span><span class="wh-cart-total"></span><div class="additional-notes"><span class="wh-minimums-note"></span><span class="wh-extra-note"></span></div></span>
        </span>
        {% endif %}
        
      </p>

      {% if section.settings.display_savings %}
      <p class="cart_item--discounts">
        {% for discount_application in cart.discount_applications %}
        {% assign total_saving = discount_application.total_allocated_amount | plus: total_saving %}
        {% endfor %}
        {% if total_saving > 0 %}
        <span>{{ 'layout.general.savings' | t }}</span>: &nbsp;&nbsp;&nbsp;
        <span class="right">
          <span class="money">{{ total_saving | money }}</span>
        </span>
        
        {% endif %}
      </p>
      {% endif %}


<!--       <div class="discount-code-field"> 
        <label for="discount" class="hidden">DISCOUNT CODE - APPLIED AT CHECKOUT</label> 
        <input autocomplete="off" type="hidden" name="discount" /> 
      </div>  -->
      {% if settings.show_multiple_currencies %}
      <p>
        <small>
          {{ 'cart.general.orders_processed_in_currency_html' | t: currency: shop.currency }}
        </small>
      </p>
      {% endif %}

          {% comment %}
          <li>
            <!-- Cart subtotal -->
            Subtotal: {{ cart.items_subtotal_price | money }}
            <!-- Cart level discounts -->
            {%- if cart.cart_level_discount_applications != blank -%}
            Discounts:
            <ul>
              {%- for discount_application in cart.cart_level_discount_applications -%}
              <li>{{- discount_application.title -}}
                -{{ discount_application.total_allocated_amount | money }}</li>
              {%- endfor -%}
            </ul>
            {%- endif -%}
            <!-- Final cart total -->
            Total: {{ cart.total_price | money }}
          </li>
          {% endcomment %}

      {% if section.settings.display_tos_checkbox %}
      <p>
        <input type="checkbox" class="tos_agree" id="cart_agree" required />
        <label class="tos_label" for="cart_agree">
          {{ 'cart.general.agree_to_terms_html' | t }}
        </label>
        {% if settings.tos_page != blank %}
        <a href="{{ pages[settings.tos_page].url }}" target="_blank" class="tos_icon">{{ 'cart.general.view_terms' | t }}</a>
        {% endif %}
      </p>
      {% endif %}

      {% if section.settings.cart_message != blank %}
      <div>
        {{ section.settings.cart_message }}
      </div>
      {% endif %}


      {% if bold_cart_total_price < min_cart %}
      <p style="font-size:1em;color:#e3332f;" class="min-cart-error">*Oops! <span class="money">£{{ settings.mincartvalue }}</span> {{ settings.mincarterror }}</p>
      {% endif %}
      {% if additional_checkout_buttons  %}
      <div class="additional-checkout-buttons">
        {{ content_for_additional_checkout_buttons }}
      </div>
      {% endif %}

      {% if cart.item_count > 0 %}
      <div class="featured-links">
        <!--                 <a href="{% if cart.items.first.product.collections != blank %}{{ cart.items.first.product.collections.last.url }}{% else %}/collections/all{% endif %}" class="secondary_button">{{ 'cart.general.continue_shopping_link_html' | t }}</a> -->        
        <p>
          <input type="submit" class="checkout-btn"  id="checkout" name="checkout" value="{{ 'cart.general.checkout' | t | upcase}}" />        
        </p>
      </div>
      {% endif %}
    </div>
    <div style="clear:both;"></div>
    </div>
   


    <!--  Product Footer Icon  -->  
    <div id="product-detail-section-tiles">
      <h2>{% if section.settings.product_icon_title != blank %} {{section.settings.product_icon_title}} {%endif%}</h2>
      <ul>
        <li>
          {% if section.settings.product_icon1_img != blank %} {{ section.settings.product_icon1_img | img_url:"master" | img_tag : 'All natural Ingredients' }}
                    <p>{% if section.settings.product_icon1_title != blank %}{{section.settings.product_icon1_title}}{%endif%}</p> 
          {% endif %}
        </li>
        <li>
          {% if section.settings.product_icon2_img != blank %} {{ section.settings.product_icon2_img | img_url:"master" | img_tag : 'Gluten Free'}}
                    <p>{% if section.settings.product_icon2_title != blank %}{{section.settings.product_icon2_title}}{%endif%}</p> 
          {% endif %}
        </li>
        <li>
          {% if section.settings.product_icon3_img != blank %} {{ section.settings.product_icon3_img | img_url:"master" | img_tag : 'Reliable 1-3 Day Delivery' }}
                    <p>{% if section.settings.product_icon3_title != blank %}{{section.settings.product_icon3_title}}{%endif%}</p> 
          {% endif %}
        </li>
        <li>
          {% if section.settings.product_icon4_img != blank %} {{ section.settings.product_icon4_img | img_url:"master" | img_tag :'Free UK shipping on orders over £17.99' }}
                    <p>{% if section.settings.product_icon4_title != blank %}{{section.settings.product_icon4_title}}{%endif%}</p> 
          {% endif %}
        </li>
        <li class="hide-mobile">
          {% if section.settings.product_icon5_img != blank %} {{ section.settings.product_icon5_img | img_url:"master" | img_tag }}
          <!--           <p>{% if section.settings.product_icon5_title != blank %}{{section.settings.product_icon5_title}}{%endif%}</p>  -->
          {% endif %}
        </li>
      </ul>
    </div>  
    <div style="clear:both;"></div>
    <!-- End Product Footer Icon  -->
  </form>
</div>
{% endif %}


