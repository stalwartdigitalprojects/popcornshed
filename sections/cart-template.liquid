<script>
  $('#empty_basket').on('click',function(e){
    e.preventDefault();
    $(this).text('Please wait...').attr('disabled','disabled');
    $.ajax({
      type: "POST",
      url: '/cart/clear.js',
      data: '',
      dataType: 'json',
      success: function() {
        $('#empty_basket').text('Empty My Basket').removeAttr('disabled');
        location.reload();
      },
      error: function(XMLHttpRequest, textStatus) {
        /* error code */
      }
    });
  })
</script>
